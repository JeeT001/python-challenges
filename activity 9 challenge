from ast import If, Str
import string

#Write a Python program that prints the table from 1 to 12. Each row should display 4 tables with proper format. 

temp = 1
for z in range(3):
    for x in range(1,11):
        for y in range(temp,temp+4):
            print("   %d X %2d = %3d" %(y, x, x*y) , end="")
        print()
    temp+=4
    print()
